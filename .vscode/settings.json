{
    "files.exclude": {
        "**/.data": true,
        "**/dist": true,
        "**/node_modules": true
    },
    "psi-header.changes-tracking": {
        "autoHeader": "autoSave",
        "excludeGlob": [
            "**.json",
            "**.md",
            "**.txt"
        ]
    },
    "psi-header.variables": [
        ["projectCreationYear", "2019"]
    ],
    "psi-header.templates": [
        {
            "language": "*",
            "template": [
                "Copyright <<projectCreationYear>> - <<year>> Nathan Alo, Ayane Satomi, et al.",
                "Licensed under the GNU General Public License v3",
                "See LICENSE for details."
            ]
        }
    ]
}